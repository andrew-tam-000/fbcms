{
    "hosting": {
        "public": "dist/build",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/_admin",
                "destination": "/_admin.html"
            },
            {
                "source": "**",
                "function": "render"
            }
        ]
    }
}
